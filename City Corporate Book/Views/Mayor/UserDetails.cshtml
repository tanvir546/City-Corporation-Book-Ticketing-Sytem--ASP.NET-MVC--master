@model City_Corporate_Book.Models.ViewModel.RegistrationVModel
@{
    ViewBag.Title = "UserDetails";
    Layout = "~/Views/Shared/_MayorLayout.cshtml";
}

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/UserDetails.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.0.0.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>


    <h1 class="row justify-content-center">USER PROFILE</h1>
    <hr />

    <div class="container">
        <div class="form-group mr-6">
            @Html.LabelFor(m => m.Email, new { @class = "col-sm-2 col-form-label" })
            @Html.DisplayFor(m => m.Email, new { @class = "col-4 form-control-sm" })
        </div>

        <div class="form-group mr-6">
            @Html.LabelFor(m => m.Location, new { @class = "col-sm-2 col-form-label" })
            @Html.DisplayFor(m => m.Location, new { @class = "col-4 form-control-sm" })
        </div>

        <div class="form-group mr-6">
            @Html.LabelFor(m => m.VoterId, new { @class = "col-sm-2 col-form-label" })
            @Html.DisplayFor(m => m.VoterId, new { @class = "col-4 form-control-sm" })
        </div>
        <div class="form-group mr-6">
            @Html.LabelFor(m => m.LoginIdentity, new { @class = "col-sm-2 col-form-label" })
            @Html.DisplayFor(m => m.LoginIdentity, new { @class = "col-sm-2 col-form-label" })
         </div>
        
    </div>





