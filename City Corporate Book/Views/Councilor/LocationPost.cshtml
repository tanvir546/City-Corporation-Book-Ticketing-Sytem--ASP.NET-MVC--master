@{
    ViewBag.Title = "MemberPost";
    Layout = "~/Views/Shared/_CouncilorLayout.cshtml";
    var Location = Request.Cookies["location"].Value.ToString(); 
}
<meta name="viewport" content="width=device-width" />
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/select2.min.js"></script>
<script src="~/Scripts/select2.js"></script>
<link href="~/Content/css/select2.css" rel="stylesheet" />
<link href="~/Content/css/select2.min.css" rel="stylesheet" />

<p id="para" hidden>@Location</p>

<div class="row">
    <div class="col-lg-4"></div>
    <div class="col-lg-4"><center><h1>Members Complains</h1></center></div>
</div>

<div id="POST">

</div>

<script>
    $(".select2").select2();
    
    $(document).ready(function () {
        //console.log("ready!");
        //$("#StoreList tbody tr").remove();
        var location = $("#para").text();
        var url = "/Councilor/WatchMembersPost/";

        $.get(url, { loca: location },
            function (result) {
                console.log(result);
                $('#POST').html(result);
            });
    });
    

</script>
